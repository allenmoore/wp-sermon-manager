  "homepage": "https://github.com/allenmoore/wp-sermon-manager",
  "license": "MIT",
  "authors": [
    {
      "name": "Allen Moore",
      "email": "am@allenmoore.co",
      "homepage": "https://allenmoore.co",
      "role": "Developer"
    }
  ],
  "support": {
    "email": "am@allenmoore.co",
    "issues": "https://github.com/allenmoore/wp-sermon-manager/issues",
    "source": "https://github.com/allenmoore/wp-sermon-manager",
    "docs": "https://github.com/allenmoore/wp-sermon-manager#readme"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
